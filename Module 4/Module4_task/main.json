{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "vnetConfig": {
            "type": "object"
        },
        "ContributorId": {
            "type": "string"
        },
        "OwnerId": {
            "type": "string"
        },
        "secretsObject": {
            "type": "secureObject"
        },
        "SAConfig": {
            "type": "object"
        },
        "virtualmachinesize":{
            "type": "string"
        },
        "adminUsername":{
            "type": "string"
        },
        "configurationFunction":{
            "type": "string"
        },
        "sastokenurl":{
            "type": "string"
        },
        "containerSasToken": {
            "type": "string" 
       }
    },
    "variables": {
        "get_link": "https://iksabase.blob.core.windows.net/ik-cont-main/linked/"
    },
    "resources": [
        {
            "apiVersion": "2015-01-01",
            "name": "NetworkingTemplate",
            "type": "Microsoft.Resources/deployments",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[concat(uri(variables('get_link'), 'vnet.json'), parameters('containerSasToken'))]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "AdressPrefix": {
                        "value": "[parameters('vnetConfig').AdressPrefix]"
                    },
                    "SubnetAdressPrefix": {
                        "value": "[parameters('vnetConfig').SubnetAdressPrefix]"
                    },
                    "VnetDdosProtection": {
                        "value": "[parameters('vnetConfig').VnetDdosProtection]"
                    },
                    "publicIpAddressType": {
                        "value": "[parameters('vnetConfig').publicIpAddressType]"
                    },
                    "publicIpAddressSku": {
                        "value": "[parameters('vnetConfig').publicIpAddressSku]"
                    }
                }
            }
        },
        {
            "apiVersion": "2015-01-01",
            "name": "SALinkedTemplate",
            "type": "Microsoft.Resources/deployments",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[concat(uri(variables('get_link'), 'sa.json'), parameters('containerSasToken'))]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "SAAccountType": {
                        "value": "[parameters('SAConfig').SAAccountType]"
                    },
                    "SAAccessTier": {
                        "value": "[parameters('SAConfig').SAAccessTier]"
                    },
                    "SAHttpsTraffic": {
                        "value": "[parameters('SAConfig').SAHttpsTraffic]"
                    },
                    "SAkind": {
                        "value": "[parameters('SAConfig').SAkind]"
                    }
                }
            }
        },
        {
            "apiVersion": "2015-01-01",
            "name": "KeyVaultLinkedTemplate",
            "type": "Microsoft.Resources/deployments",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[concat(uri(variables('get_link'), 'KeyVault.json'), parameters('containerSasToken'))]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "ContributorId": {
                        "value": "[parameters('ContributorId')]"
                    },
                    "OwnerId": {
                        "value": "[parameters('OwnerId')]"
                    },
                    "secretsObject": {
                        "value": "[parameters('secretsObject')]"
                    }
                }
            }
        },
        {
            "apiVersion": "2015-01-01",
            "name": "VirtMachineLinkedTemplate",
            "type": "Microsoft.Resources/deployments",
            "dependsOn": [
                "KeyVaultLinkedTemplate",
                "SALinkedTemplate",
                "NetworkingTemplate"
            ],
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[concat(uri(variables('get_link'), 'Win2016_template.json'), parameters('containerSasToken'))]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "virtualMachineSize": {
                        "value": "[parameters('virtualmachinesize')]"
                    },
                    "nicName": {
                        "value": "[reference('NetworkingTemplate').outputs.nicName.value]"
                    },
                    "AdminPassword": {
                        "value": "[reference('KeyVaultLinkedTemplate').outputs.AdminPassword.value]"
                    },
                    "adminUsername":{
                        "value": "[parameters('adminUsername')]"
                    },
                    "configurationFunction":{
                        "value": "[parameters('configurationFunction')]"
                    },
                    "sastokenurl":{
                        "value": "[parameters('sastokenurl')]"
                    }
                }
            }
        }
    ]
}